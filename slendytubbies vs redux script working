local A1Button = A1Tab:CreateButton({
   Name = "light CUSTERE",
   Callback = function()
   local esptogglekeybind = Enum.KeyCode.B
local espcolor = Color3.fromRGB(255, 0, 0)
local glowcolor = Color3.fromRGB(255, 0, 0)

--options above and code below!


local onoff = true
game:GetService("UserInputService").InputBegan:Connect(function(v)
    if v.KeyCode == esptogglekeybind then
        if onoff == true then
            onoff = false
        elseif onoff == false then
            onoff = true
        end
    end
end)
while wait() do
    if onoff == true then
        for i, v in pairs(game:GetService("Workspace").game.gameCustard:GetChildren()) do
            if v:FindFirstChild("hax1") == nil then
                local Highlight = Instance.new("Highlight")
                local PointLight = Instance.new("PointLight")
                Highlight.OutlineTransparency = 10
                Highlight.FillTransparency = 0.6
                Highlight.FillColor = espcolor
                Highlight.Name = "hax1"
                PointLight.Color = glowcolor
                PointLight.Brightness = 100
                PointLight.Range = 6.5
                PointLight.Name = "hax2"
                Highlight.Parent = v
                PointLight.Parent = v
                Highlight.Adornee = v
            end
        end
    elseif onoff == false then
        for i, v in pairs(game:GetService("Workspace").game.gameCustard:GetChildren()) do
            if v:FindFirstChild("hax1") ~= nil then
                v["hax1"]:Destroy()
            end
            if v:FindFirstChild("hax2") ~= nil then
                v["hax2"]:Destroy()
            end
        end
    end
end
   end,
})

